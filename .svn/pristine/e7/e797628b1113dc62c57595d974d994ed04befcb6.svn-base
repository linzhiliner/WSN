<?xml version="1.0" encoding="GBK"?>
<!-- 指定Hibernate映射文件的DTD信息 -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Hibernate映射文件的根元素 -->
<hibernate-mapping package="com.huawei.domain">
	<class	name="UserType"	table="tb_UserType">
		<!-- 映射标识属性 -->
		<id	name="id" type="long" column="id">
			<!-- 指定使用identity主键生成策略 -->
			<generator class="native"/>
		</id>
	    <!-- 映射普通属性 -->
        <property name="dateCreated" column="createtime" type = "timestamp"/>
        <property name="typeName" column="typename" type="string" not-null="true" />
	</class>
</hibernate-mapping>