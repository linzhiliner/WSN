package com.huawei.domain;

import java.io.Serializable;
import java.util.HashSet;
import java.util.Set;

public class Focus extends BaseDomainObject implements Serializable {
	private static final long serialVersionUID = 48L;
	private RegisterUser owner;// 被关注者
	private RegisterUser idol;// 关注者
	private Integer userLevel;


	public RegisterUser getOwner() {
		return owner;
	}

	public void setOwner(RegisterUser owner) {
		this.owner = owner;
	}

	public RegisterUser getIdol() {
		return idol;
	}

	public void setIdol(RegisterUser idol) {
		this.idol = idol;
	}

	public Integer getUserLevel() {
		return userLevel;
	}

	public void setUserLevel(Integer userLevel) {
		this.userLevel = userLevel;
	}

}
